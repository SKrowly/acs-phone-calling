# Azure Communication Services Configuration
# Get your connection string from Azure Portal > Communication Services > Keys
ACS_CONNECTION_STRING=endpoint=https://your-acs-resource.communication.azure.com/;accesskey=your_access_key_here

# Phone numbers in E.164 format (e.g., +12125551234)
TO_PHONE_NUMBER=+1234567890
FROM_PHONE_NUMBER=+1987654321

# Webhook callback URL for receiving call events
# Use ngrok or your public endpoint (e.g., https://your-domain.com/api/callbacks)
CALLBACK_URI=https://your-webhook-url.com/api/callbacks
